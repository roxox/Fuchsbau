    {#<!-- CSS code from Bootply.com editor -->#}

    <style type="text/css">
        /* bootstrap 3 helpers */

        /* bootstrap 3 helpers */

        .navbar-form input, .form-inline input {
            width:auto;
        }

        .navbar {
            margin-bottom:0;
        }

        .subnav {
            margin:0;
            top: 0px;
            z-index: 1020;
            /*background-color: rgb(247,247,247);*/
            background-color: transparent;
            /*border-bottom: 2px solid #E1E1E1;*/
            padding: 60px 0px 0px 0px;
            /*padding-top: 60px;*/
        }

        .subnav.affix {
            position: fixed;
            top: 0;
            width: 100%;
            z-index:10;
        }

        .subnav .navbar-brand:hover,
        .subnav .navbar-brand:focus {
            color: #fff;
            background-color: transparent;
        }
        .subnav .navbar-text {
            color: #fff;
        }
        .subnav .navbar-nav > li > a {
            color: #999;
        }
        .subnav .navbar-nav > li > a:hover,
        .subnav .navbar-nav > li > a:focus {
            color: #fff;
            background-color: transparent;
        }

        /* LINK FX */
        .btn-linkfx {
            font-weight: normal;
            color: #61625d;
            cursor: pointer;
            border-radius: 0;
        }
        .btn-linkfx,
        .btn-linkfx:active,
        .btn-linkfx[disabled],
        fieldset[disabled] .btn-link {
            background-color: transparent;
            -webkit-box-shadow: none;
            box-shadow: none;
        }
        .btn-linkfx,
        .btn-linkfx:hover,
        .btn-linkfx:focus,
        .btn-linkfx:active {
            border-color: transparent;
        }
        .btn-linkfx:hover,
        .btn-linkfx:focus {
            color: #4e4f4b;
            font-weight: bold;
            text-decoration: none;
            background-color: transparent;
        }
        .btn-linkfx[disabled]:hover,
        fieldset[disabled] .btn-link:hover,
        .btn-linkfx[disabled]:focus,
        fieldset[disabled] .btn-link:focus {
            color: #999;
            text-decoration: none;
        }

        /* LINK FX RED*/
        .btn-linkfxred {
            font-weight: normal;
            color: #d0171f;
            cursor: pointer;
            border-radius: 0;
        }
        .btn-linkfxred,
        .btn-linkfxred:active,
        .btn-linkfxred[disabled],
        fieldset[disabled] .btn-link {
            background-color: transparent;
            -webkit-box-shadow: none;
            box-shadow: none;
        }
        .btn-linkfxred,
        .btn-linkfxred:hover,
        .btn-linkfxred:focus,
        .btn-linkfxred:active {
            border-color: transparent;
        }
        .btn-linkfxred:hover,
        .btn-linkfxred:focus {
            color: #b6141b;
            font-weight: bold;
            text-decoration: none;
            background-color: transparent;
        }
        .btn-linkfxred[disabled]:hover,
        fieldset[disabled] .btn-link:hover,
        .btn-linkfxred[disabled]:focus,
        fieldset[disabled] .btn-link:focus {
            color: #999;
            text-decoration: none;
        }

        /* LINK FX BRIGHTBLUR*/
        .btn-link-fx-brightblue {
            font-weight: normal;
            color: #6aa2c3;
            cursor: pointer;
            border-radius: 0;
        }
        .btn-fx-link-fx-brightblue,
        .btn-fx-link-fx-brightblue:active,
        .btn-fx-link-fx-brightblue[disabled],
        fieldset[disabled] .btn-link-fx-brightblue {
            background-color: transparent;
            -webkit-box-shadow: none;
            box-shadow: none;
        }
        .btn-fx-link-fx-brightblue,
        .btn-fx-link-fx-brightblue:hover,
        .btn-link-fx-brightblue:focus,
        .btn-link-fx-brightblue:active {
            border-color: transparent;
        }
        .btn-link-fx-brightblue:hover,
        .btn-link-fx-brightblue:focus {
            color: #55829d;
            text-decoration: none;
            background-color: transparent;
            font-weight: bold;
            text-decoration: none;
        }
        .btn-link-fx-brightblue[disabled]:hover,
        fieldset[disabled] .btn-link-fx-brightblue:hover,
        .link-fx-brightblue-linkfxred[disabled]:focus,
        fieldset[disabled] .link-fx-brightblue-link:focus {
            color: #999;
            text-decoration: none;
        }

        /* BTN DEFAULT RED*/
        .btn-foxy {
            color: #333;
            background-color: #fff;
            border-color: #ccc;
        }
        .btn-foxy:hover,
        .btn-foxy:focus,
        .btn-foxy:active,
        .btn-foxy.active,
        .open .dropdown-toggle.btn-foxy {
            color: #333;
            background-color: #ebebeb;
            border-color: #adadad;
        }
        .btn-foxy:active,
        .btn-foxy.active,
        .open .dropdown-toggle.btn-foxy {
            background-image: none;
        }
        .btn-foxy.disabled,
        .btn-foxy[disabled],
        fieldset[disabled] .btn-foxy,
        .btn-foxy.disabled:hover,
        .btn-foxy[disabled]:hover,
        fieldset[disabled] .btn-foxy:hover,
        .btn-foxy.disabled:focus,
        .btn-foxy[disabled]:focus,
        fieldset[disabled] .btn-foxy:focus,
        .btn-foxy.disabled:active,
        .btn-foxy[disabled]:active,
        fieldset[disabled] .btn-foxy:active,
        .btn-foxy.disabled.active,
        .btn-foxy[disabled].active,
        fieldset[disabled] .btn-foxy.active {
            background-color: #fff;
            border-color: #ccc;
        }
        .btn-foxy .badge {
            color: #fff;
            background-color: #333;
        }

        .subnav .navbar-link {
            color: #999;
        }
        .subnav .navbar-link:hover {
            color: #fff;
        }

    </style>

</head>
<body>
<div>
    {% block topnav %}
        {% include 'topnav.html.twig' %}
    {% endblock %}
</div>
<br><br><br>

<div class="container">
    <div>
        {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
            {#{% block content %}{% endblock %}#}

            {#{{ 'layout.logged_in_as'|trans({'%username%': app.user.username}, 'FOSUserBundle') }} |#}
            {#<a href="{{ path('fos_user_security_logout') }}">#}
            {#{{ 'layout.logout'|trans({}, 'FOSUserBundle') }}#}
            {#</a>#}
        {% else %}

            {#{% include "ausgeloggt.html.twig" %}#}
            {#<a href="{{ path('fos_user_security_login') }}">{{ 'layout.login'|trans({}, 'FOSUserBundle') }}</a>#}
        {% endif %}
    </div>

    {% block content %}{% endblock %}
    <div class="container">
        <hr>

        {#<footer>#}
            {#<p>&copy; 2016 Sebastian Fox</p>#}
        {#</footer>#}

<br><br><br>
        <div>
            {% block bottomnav %}
                {% include 'bottomnav.html.twig' %}
            {% endblock %}
        </div>
    </div>
</div>


